{

  "manifest_version": 2,
  "name": "notes",
  "version": "1.0",

  "description": "saves notes",

  "icons": {
    "48": "icons/addonicon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["notes.js"]
    }
  ],
  "browser_action": {
  "browser_style": true,
  "default_icon": {
    "16": "button/geo-16.png",
    "32": "button/geo-32.png"
  },
  "default_title": "Whereami?",
  "default_popup": "popup/geo.html",
  "theme_icons": [{
    "light": "icons/geo-32-light.png",
    "dark": "icons/geo-32.png",
    "size": 32
  }]
}

}
